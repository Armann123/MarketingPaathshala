<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.digitalmarketingschool">

    <uses-permission android:name="android.permission.INTERNET" />

    <queries>
        <intent>
            <action android:name="com.google.android.youtube.api.service.START" />
        </intent>
    </queries>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Digitalmarketingschool">
        <activity android:name=".Contentmarketing"></activity>
        <activity android:name=".Whataffiliatemarketing" />
        <activity android:name=".Googleads1" />
        <activity android:name=".LandingPage" />
        <activity android:name=".Googlesitemap" />
        <activity android:name=".Instagrammarketing" />
        <activity android:name=".Emailmarketing" />
        <activity android:name=".Niche" />
        <activity android:name=".Socialmedia" />
        <activity android:name=".WebHosting" />
        <activity android:name=".SEO" />
        <activity android:name=".forgotpassword" />
        <activity android:name=".registrationform" />
        <activity android:name=".paidmarketing" />
        <activity android:name=".Backlinks" />
        <activity android:name=".Email" />
        <activity android:name=".Blogging" />
        <activity android:name=".Affiliatefield" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>